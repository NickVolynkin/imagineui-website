msgid "Data types"
msgstr "Типы данных"

#: 3143e8f8e75c436b94859945538e21f6
msgid "Syntax"
msgstr "Синтаксис"

#: c9cb1bdf9b7f4aaa9534451e55870327
msgid "Aligned"
msgstr "Расположены"

#: 091524891bfc42588a3a42f40c7a58c4
msgid "Use this keyword to align elements in a block:"
msgstr "Используйте это ключевое слово чтобы задать расположение элементов в блоке:"

#: 54dc520215dd44a2ad669638e5f3886a
msgid "Block"
msgstr "Блок"

#: 1ff9b8f778e14ec4acf19caa9c446265 356286e368224d01b5c597f59bdd60b1
#: 4cb65141e5144d59a5f7114b4f7b7c0e 556a60dfdfe745b4aa42be6db8b028c9
#: 8c1b82109e6c4ebaaa6facf6a9288c66 9307b54fa3e248128e7bec70579ca655
#: afa11c112b5149c6aecf114cb88129c9 d52a47693ad04b1997e45b8f6957936b
